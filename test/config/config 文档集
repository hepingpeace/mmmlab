#config_a.py

a = 1
b = dict(b1=[0, 1, 2], b2=None)


#config_b.py

_base_ = './config_a.py'
c = (1, 2)
d = 'string'



# config_c.py

_base_ = './config_a.py'
b = dict(b2=1)
c = (1, 2)



# config_d.py

_base_ = './config_a.py'
b = dict(_delete_=True, b2=None, b3=0.1)
c = (1, 2)


# config 文档集.py

c = (1, 2)
d = 'string'


# config_f.py

_base_ = ['./config_a.py', './config 文档集.py']


# base.py

item1 = 'a'
item2 = dict(item3 = 'b')


# config_g.py

_base_ = ['./base.py']
item = dict(a = {{ _base_.item1 }}, b = {{ _base_.item2.item3 }})


# deprecated_cfg.py

_base_ = 'expected_cfg.py'

_deprecation_ = dict(
    expected = 'expected_cfg.py',  # optional to show expected config path in the warning information
    reference = 'url to related PR'  # optional to show reference link in the warning information
)